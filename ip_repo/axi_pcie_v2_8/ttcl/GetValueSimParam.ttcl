<:       set gt_path "pcie_top_with_gt_top.gt_ies.gt_top_i"  :>
<:       set gen_ies_gt_wrappers  [expr {(true)?true:false}] :>
<:    } :>
<: } else {  :>
<: set gt_path "pcie_top_with_gt_top.gt_ges.gt_top_i"  :>
<: }  :>
