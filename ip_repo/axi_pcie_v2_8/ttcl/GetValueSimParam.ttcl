<: set x0y0_ln0 7 :>
<: set x0y0_xucf 9 :>
<: set x0y0_yucf 30 :>
<: } :>
<: if {[starts_with $prj_dev "xc7k420t"] || [starts_with $prj_dev "xc7k420tl"]} {  :>
<: set x0y0_ln0 19 :>
<: set x0y0_xucf 10 :>
<: set x0y0_yucf 40 :>
<: } :>
<: if {[starts_with $prj_dev "xc7k480t"] || [starts_with $prj_dev "xc7k480tl"]} {  :>
<: set x0y0_ln0 19 :>
<: set x0y0_xucf 10 :>
<: set x0y0_yucf 40 :>
