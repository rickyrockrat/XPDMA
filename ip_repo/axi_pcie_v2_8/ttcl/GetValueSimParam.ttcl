<: set ext_ch_gt_drp_string      [getStringValue "ext_ch_gt_drp"] :>
<: set transceiver_ctrl_status_ports_string [getStringValue "transceiver_ctrl_status_ports"] :>
<: set ext_pipe_interface_string [getStringValue "ext_pipe_interface"] :>
