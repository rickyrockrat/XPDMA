<: ;#--------------------------:>
<: ;#  Set up UCF Generation   :>
<: ;#--------------------------:>
<: ;# blk_locn | PCIe Location :>
<: ;#    0     |     X0Y0      :>
<: ;#    1     |     X0Y1      :>
<: ;#    2     |     X0Y2      :>
<: ;#    3     |     X1Y0      :>
<: ;#    4     |     X1Y1      :>
<: ;#--------------------------:>
<: set gen_x0y0_ucf [expr {(false)?true:false}]  :>
<: set gen_x0y1_ucf [expr {(false)?true:false}]  :>
<: set gen_x0y2_ucf [expr {(false)?true:false}]  :>
<: set gen_x1y0_ucf [expr {(false)?true:false}]  :>
<: set gen_x1y1_ucf [expr {(false)?true:false}]  :>
<: set design_name "xilinx_axi_pcie_7x_ep" :>
<: set lnk_spd "" :>
<: set ucf_name "axi_pcie" :>
<: set xdc_name "xilinx_axi_pcie" :>
