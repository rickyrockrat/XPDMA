<: send_dbg_msg 1 "PCIE BLK LOCN=$c_pcie_blk_locn_int" :>
<: ;#------------------------------------------------------ :>
<: ;# Set the Lane 0 XY Coordinates.                        :>
<: ;# Default to Invalid Coord to ensure no false positives :>
<: ;# Copied directory from PCIe GEN2 Streaming GVSP.ttcl   :>
<: ;#------------------------------------------------------ :>
<: set x0y0_ln0 99   :>
<: set x0y1_ln0 99   :>
<: set x0y2_ln0 99   :>
<: set x1y0_ln0 99   :>
<: set x1y1_ln0 99   :>
<: set x0y3_xucf 999 :>
<: set x0y3_yucf 999 :>
<: set x0y2_xucf 999 :>
<: set x0y2_yucf 999 :>
<: set x0y1_xucf 999 :>
<: set x0y1_yucf 999 :>
<: set x0y0_xucf 999 :>
<: set x0y0_yucf 999 :>
<: set x1y0_xucf 999 :>
<: set x1y0_yucf 999 :>
<: set x1y1_xucf 999 :>
<: set x1y1_yucf 999 :>
<: ;#Set BRAM and GT LOC's for Artix-7 Devices :>
<: if {[starts_with $prj_fam "artix7"] || [starts_with $prj_fam "qartix7"] || [starts_with $prj_fam "aartix7"]} {  :>
<: if {[starts_with $prj_dev "xc7a35t"] || [starts_with $prj_dev "xc7a50t"] || [starts_with $prj_dev "xa7a35t"] || [starts_with $prj_dev "xa7a50t"] || [starts_with $prj_dev "xq7a50t"] || [starts_with $prj_dev "xc7a15t"] || [starts_with $prj_dev "xa7a15t"]} {  :>
<: if {[starts_with $prj_pkg "cpg236"]} { :>
<: set x0y0_ln0 1 :>
<: } else { :>
<: set x0y0_ln0 3 :>
<: } :>
<: } else { :>
<: set x0y0_ln0 7 :>
<: } :>
<: if {[starts_with $prj_dev "xc7a100t"] || [starts_with $prj_dev "xq7a100t"] || [starts_with $prj_dev "xa7a100t"] || [starts_with $prj_dev "xc7a75t"] || [starts_with $prj_dev "xa7a75t"]} {  :>
<: set x0y0_xucf 1 :>
<: set x0y0_yucf 31 :>
<: } elseif {[starts_with $prj_dev "xc7a35t"] || [starts_with $prj_dev "xc7a50t"] || [starts_with $prj_dev "xa7a35t"] || [starts_with $prj_dev "xa7a50t"] || [starts_with $prj_dev "xq7a50t"] || [starts_with $prj_dev "xc7a15t"] || [starts_with $prj_dev "xa7a15t"]} {  :>
<: set x0y0_xucf 0 :>
<: set x0y0_yucf 20 :>
<: } elseif  {[starts_with $prj_dev "xc7a200t"] || [starts_with $prj_dev "xq7a200t"]}  { :>
<: set x0y0_xucf 1 :>
<: set x0y0_yucf 41 :>
<: } else { :>
<: set x0y0_xucf 999 :>
<: set x0y0_yucf 999 :>
<: } :>
<: } :>
<: ;#Set BRAM and GT LOC's for Kintex7 Devices :>
<: if {[starts_with $prj_dev "xc7k70t"] || [starts_with $prj_dev "xc7k70tl"] } {  :>
