<: set include_rc                [getIntValue "C_INCLUDE_RC"] :>
<: set pci_exp_ref_freq_int      [getIntValue "C_REF_CLK_FREQ"] :>
<: set pci_exp_ref_freq_string   [getStringValue "C_REF_CLK_FREQ"] :>
<: set max_lnk_spd_int           [getIntValue "C_MAX_LINK_SPEED"] :>
<: set max_lnk_wdt_int           [getIntValue "C_NO_OF_LANES"] :>
<: set pcie_axi_if_wdt_int       [getIntValue "C_S_AXI_DATA_WIDTH"] :>
