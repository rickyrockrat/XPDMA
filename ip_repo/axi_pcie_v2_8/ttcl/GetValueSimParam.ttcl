<: } else { :>
<: set x4g1 [expr {(true)?true:false}]  :>
<: } :>
<: } :>
<: if {$max_lnk_wdt_int == 2} {  :>
<: if {$max_lnk_spd_int == 1} {  :>
<: set x2g2 [expr {(true)?true:false}]  :>
<: } else { :>
<: set x2g1 [expr {(true)?true:false}]  :>
<: } :>
<: } :>
<: if {$max_lnk_wdt_int == 1} {  :>
<: if {$max_lnk_spd_int == 1} {  :>
<: set x1g2 [expr {(true)?true:false}]  :>
