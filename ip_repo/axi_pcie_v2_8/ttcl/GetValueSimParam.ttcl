<: ;#------------------------------------------------------ :>
<: ;# Set the path to the GT Channel                        :>
<: ;#------------------------------------------------------ :>
<: set gt_channel_path "" :>
<: set gt_channel_type "" :>
<: set gt_device "" :>
<: if {[starts_with $prj_fam "artix7"] || [starts_with $prj_fam "qartix7"] || [starts_with $prj_fam "aartix7"] || ([starts_with $prj_fam "zynq"] && [starts_with $prj_dev "xc7z015"])} {  :>
<: set gt_channel_path "gtp_channel.gtpe2_channel_i" :>
<: set gt_channel_type "GTPE2" :>
<: set gt_device "GTP" :>
<: } else { :>
<: set gt_channel_path "gtx_channel.gtxe2_channel_i" :>
<: set gt_channel_type "GTXE2" :>
<: set gt_device "GTX" :>
<: } :>
<: if {$pcie_ext_clk_string == "FALSE"}  { :>
<: set pipe_clk_path [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat "" $design_path] "/"] $gt_path] "/pipe_wrapper_i/pipe_clock_int."] :>
<: } else { :>
<: set pipe_clk_path [ttcl_string_concat $ComponentName_string "_support_i/"] :>
<: } :>
<: set ref_clk_freq 100 :>
<: set ref_clk_prd 10 :>
<: if {$pci_exp_ref_freq_int == 0} {  :>
<: set ref_clk_freq 100 :>
<: set ref_clk_prd 10 :>
<: } elseif  {($pci_exp_ref_freq_int == 1)} {  :>
<: set ref_clk_freq 125 :>
<: set ref_clk_prd 8 :>
<: } else { :>
<: set ref_clk_freq 250 :>
<: set ref_clk_prd 4 :>
<: } :>
<: if {$max_lnk_spd_int == 1} {  :>
<: set lnk_spd "g2" :>
<: } else { :>
<: set lnk_spd "g1" :>
<: } :>
<: if {($c_pcie_blk_locn_int == 0)} {  :>
<: set xdc_name [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat "" $design_name] "_x"] $max_lnk_wdt_string] $lnk_spd]] "_X0Y0"]  :>
<: set gen_x0y0_ucf [expr {(true)?true:false}]  :>
<: set ucf_name "axi_pcie_X0Y0" :>
<: set pcie_blk "X0Y0"  :>
<: } elseif  {($c_pcie_blk_locn_int == 1)} {  :>
<: set xdc_name [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat "" $design_name] "_x"] $max_lnk_wdt_string] $lnk_spd]] "_X0Y1"]  :>
<: set gen_x0y1_ucf [expr {(true)?true:false}]  :>
<: set ucf_name "axi_pcie_X0Y1"  :>
<: set pcie_blk "X0Y1"  :>
<: } elseif  {($c_pcie_blk_locn_int == 2)} {  :>
<: set xdc_name [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat "" $design_name] "_x"] $max_lnk_wdt_string] $lnk_spd]] "_X0Y2"]  :>
<: set gen_x0y2_ucf [expr {(true)?true:false}]  :>
<: set ucf_name "axi_pcie_X0Y2"  :>
<: set pcie_blk "X0Y2"  :>
<: } elseif  {($c_pcie_blk_locn_int == 3)} {  :>
<: set xdc_name [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat "" $design_name] "_x"] $max_lnk_wdt_string] $lnk_spd]] "_X1Y0"]  :>
<: set gen_x1y0_ucf [expr {(true)?true:false}]  :>
<: set ucf_name "axi_pcie_X1Y0"  :>
<: set pcie_blk "X1Y0"  :>
<: } else { :>
<: set xdc_name [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat [ttcl_string_concat "" $design_name] "_x"] $max_lnk_wdt_string] $lnk_spd] ] "_X1Y1"]  :>
<: set gen_x1y1_ucf [expr {(true)?true:false}]  :>
<: set ucf_name "axi_pcie_X1Y1"  :>
<: set pcie_blk "X1Y1"  :>
<: } :>
