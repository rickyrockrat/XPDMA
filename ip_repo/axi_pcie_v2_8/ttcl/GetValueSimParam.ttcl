<: set xlnx_ref_board_lcd_display [expr {(true)?true:false}]  :>
<: } else { :>
<: set xlnx_ref_board_lcd_display [expr {(false)?true:false}]  :>
<: } :>
<: if {[string equal -nocase $c_xlnx_ref_board_string "KC705_REVA"] || [string equal -nocase $c_xlnx_ref_board_string "KC705_REVB"] || [string equal -nocase $c_xlnx_ref_board_string "KC705_REVC"] || [string equal -nocase $c_xlnx_ref_board_string "VC707"] || [string equal -nocase $c_xlnx_ref_board_string "AC701"]} {  :>
<: set xlnx_ref_board_ucf [expr {(true)?true:false}]  :>
<: if {[string equal -nocase $c_xlnx_ref_board_string "KC705_REVA"]} {  :>
<: set xlnx_ref_board_kc705_reva [expr {(true)?true:false}]  :>
<: } :>
<: if {[string equal -nocase $c_xlnx_ref_board_string "KC705_REVB"]} {  :>
<: set xlnx_ref_board_kc705_revb [expr {(true)?true:false}]  :>
<: } :>
<: if {[string equal -nocase $c_xlnx_ref_board_string "KC705_REVC"]} {  :>
<: set xlnx_ref_board_kc705_revc [expr {(true)?true:false}]  :>
<: } :>
<: } :>
<: if {[string equal -nocase $c_xlnx_ref_board_string "VC707"]} {  :>
<: set xlnx_ref_board_vc707 [expr {(true)?true:false}]  :>
<: } :>
<: if {[string equal -nocase $c_xlnx_ref_board_string "AC701"]} {  :>
<: set xlnx_ref_board_ac701 [expr {(true)?true:false}]  :>
<: } :>
<: if {[string equal -nocase $c_xlnx_ref_board_string "ZC706"]} {  :>
