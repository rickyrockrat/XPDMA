<: if {[starts_with $prj_dev "xc7v585t"] || [starts_with $prj_dev "xq7v585t"] } {  :>
<: set x0y0_ln0 7 :>
<: set x0y1_ln0 19 :>
<: set x0y2_ln0 31 :>
<: set x0y2_xucf 7 :>
<: set x0y2_yucf 70 :>
<: set x0y1_xucf 7 :>
<: set x0y1_yucf 40 :>
<: set x0y0_xucf 7 :>
<: set x0y0_yucf 10 :>
<: } :>
<: if {[starts_with $prj_dev "xc7v1500t"]} {  :>
