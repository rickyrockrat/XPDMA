<: } :>
<: } :>
<: set xlnx_ref_board_ucf [expr {(false)?true:false}]  :>
<: set xlnx_ref_board_kc705_reva [expr {(false)?true:false}]  :>
<: set xlnx_ref_board_kc705_revb [expr {(false)?true:false}]  :>
<: set xlnx_ref_board_kc705_revc [expr {(false)?true:false}]  :>
<: set xlnx_ref_board_vc707 [expr {(false)?true:false}]  :>
<: set xlnx_ref_board_ac701 [expr {(false)?true:false}]  :>
<: set xlnx_ref_board_zc706 [expr {(false)?true:false}]  :>
<: set xlnx_ref_board_lcd_display [expr {(false)?true:false}]  :>
<: if {([string equal -nocase $c_xlnx_ref_board_string "KC705_REVA"] || [string equal -nocase $c_xlnx_ref_board_string "KC705_REVB"] || [string equal -nocase $c_xlnx_ref_board_string "KC705_REVC"]) && [starts_with $ComponentName "enable_lcd"] } {  :>
